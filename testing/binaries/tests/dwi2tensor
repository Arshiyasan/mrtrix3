mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - - | testing_diff_image - dwi2tensor/out.mif -frac 1e-5
mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - -mask mask.mif - | testing_diff_image - dwi2tensor/out_mask.mif -frac 1e-5
mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - -mask mask.mif -b0 - tmp_dt.mif | testing_diff_image - dwi2tensor/out_b0.mif -frac 1e-5
mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - -mask mask.mif -ols -iter 0 - | testing_diff_image - dwi2tensor/out_ols_it0.mif -frac 1e-5
mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - -mask mask.mif -iter 0 - | testing_diff_image - dwi2tensor/out_it0.mif -frac 1e-5
mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - -mask mask.mif -iter 1 - | testing_diff_image - dwi2tensor/out_it1.mif -frac 1e-5
mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - -mask mask.mif -iter 2 - | testing_diff_image - dwi2tensor/out_it2.mif -frac 1e-5
mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - -mask mask.mif -iter 3 - | testing_diff_image - dwi2tensor/out_it3.mif -frac 1e-5
mrconvert dwi.mif -bvalue_scaling 1 - | dwi2tensor - -mask mask.mif -iter 4 - | testing_diff_image - dwi2tensor/out_it4.mif -frac 1e-5
